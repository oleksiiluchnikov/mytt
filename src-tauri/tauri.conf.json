{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "mytt",
  "mainBinaryName": "mytt",
  "version": "0.0.0",
  "identifier": "com.oleksiiluchnikov.mytt",
  "plugins": {},
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "fullscreen": false,
        "alwaysOnTop": true,
        "titleBarStyle": "Visible",
        "decorations": true,
        "hiddenTitle": true,
        "acceptFirstMouse": true,
        "width": 160,
        "minWidth": 160,
        "resizable": false,
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}
